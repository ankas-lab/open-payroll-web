{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"],
      "useink/chains": ["./node_modules/useink/types/chains/mod.d.ts"],
      "useink/notifications": ["./node_modules/useink/types/notifications/mod.d.ts"],
      "useink/utils": ["./node_modules/useink/types/utils/mod.d.ts"],
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/pages/create/index.tsx"
  ],
  "exclude": ["node_modules"],
}
